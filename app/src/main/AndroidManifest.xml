        </activity>

        <meta-data
            android:name="DATABASE"
            android:value=" sugar_example.db" />
        <meta-data
            android:name="VERSION"
            android:value="1" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="false" />
        <meta-data
            android:name="APP_CONTEST_3"
            android:value="com.lifeistech.naoto.myapplication_app_contest" />

        <activity android:name=".SolveActivity" />
        <activity android:name=".AnswerActivity" />
        <activity android:name=".SetUpActivity" />
        <activity android:name=".ListActivity" />
        <activity android:name=".ListGroupWordsActivity" />
        <activity
            android:name=".Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SolveWeakActivity" />
        <activity android:name=".Solve2Activity" />
